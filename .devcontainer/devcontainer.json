{
    "name": "home-qrcode-generator",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".",
    },
    "settings": {
        "terminal.integrated.profiles.linux": {
            "bash": {
                "path": "/bin/bash"
            },
        },
        "terminal.integrated.defaultProfile.linux": "bash",
        "editor.tabSize": 2,
    },
    "extensions": [
        "visualstudioexptteam.vscodeintellicode",
        "ms-python.python",
    ],
    "updateRemoteUserUID": true,
    "remoteUser": "vscode",
}